@{
    ViewData["Title"] = "Home Page";
}
<!-- ======= Hero Section ======= -->
<section id="hero" class="d-flex align-items-center">


    <div class="container" data-aos="zoom-out" data-aos-delay="100">
        <div class="row">
            <div class="col-xl-6">
                <h1>Dotacje Unijne na rok 2022</h1>
                <h2>Prowadzimy zapisy na dotacje Unijne z rożnych gatunków życia </h2>
                <a href="#about" class="btn-get-started scrollto">Czym się zajmujemy </a>
            </div>
        </div>
    </div>

</section><!-- End Hero -->

<main id="main">
    <!-- ======= About Section ======= -->
    <section id="about" class="about section-bg">
        <div class="container" data-aos="fade-up">

            <div class="row no-gutters">
                <div class="content col-xl-5 d-flex align-items-stretch">
                    <div class="content">
                        <h3>Pomagamy uzyskać dofinasowanie z następujących dziedziń</h3>
                        <p>
                            
                        </p>
                        <a href="#" class="about-btn"><span> Firma </span> <i class="bx bx-chevron-right"></i></a>
                    </div>
                </div>
                <div class="col-xl-7 d-flex align-items-stretch">
                    <div class="icon-boxes d-flex flex-column justify-content-center">
                        <div class="row">
                            <div class="col-md-6 icon-box" data-aos="fade-up" data-aos-delay="100">
                                <i class="bx bx-receipt"></i>
                                <h4>Rolnictwo</h4>
                                <p>Pomagamy uzyskać dofinasowania unijne w zakresie roślin strączkowych na ziarno, roślin pastewnych, buraków cukrowych, ziemniaków skrobiowych, pomidorów, truskawek, chmielu, lnu oraz konopi włóknistych</p>
                            </div>
                            <div class="col-md-6 icon-box" data-aos="fade-up" data-aos-delay="200">
                                <i class="bx bx-cube-alt"></i>
                                <h4>Działalność gospodarcza</h4>
                                <p>Udzielamy porad jak otrzymać dofinansowanie w ramach uruchamiana swojej pierwszej firmy</p>
                            </div>
                            <div class="col-md-6 icon-box" data-aos="fade-up" data-aos-delay="300">
                                <i class="bx bx-images"></i>
                                <h4>Szkolnictwo</h4>
                                <p>W ramach współpracy z lokalnymi szkołami pomagamy uzyskać dotacje na nowy sprzęt komputerowy  oraz wszelakie materiały pomocne w nauce oraz na hale sportowe </p>
                            </div>
                            <div class="col-md-6 icon-box" data-aos="fade-up" data-aos-delay="400">
                                <i class="bx bx-shield"></i>
                                <h4>Infrastruktura</h4>
                                <p>Udzielamy wspracia ośrodkom gminnym jak otrzymać dofinansowanie na lokalne drogi chodni itp aspekty</p>
                            </div>
                        </div>
                    </div><!-- End .content-->
                </div>
            </div>

        </div>
    </section><!-- End About Section -->
    <!-- ======= Counts Section ======= -->
    <section id="counts" class="counts">
        <div class="container" data-aos="fade-up">

            <div class="row">

                <div class="col-lg-3 col-md-6">
                    <div class="count-box">
                        <i class="bi bi-emoji-smile"></i>
                        <span data-purecounter-start="0" data-purecounter-end="232" data-purecounter-duration="1" class="purecounter"></span>
                        <p>Zadowoleni klienci </p>
                    </div>
                </div>

                <div class="col-lg-3 col-md-6 mt-5 mt-md-0">
                    <div class="count-box">
                        <i class="bi bi-journal-richtext"></i>
                        <span data-purecounter-start="0" data-purecounter-end="521" data-purecounter-duration="1" class="purecounter"></span>
                        <p>Projekty </p>
                    </div>
                </div>

                <div class="col-lg-3 col-md-6 mt-5 mt-lg-0">
                    <div class="count-box">
                        <i class="bi bi-headset"></i>
                        <span data-purecounter-start="0" data-purecounter-end="1463" data-purecounter-duration="1" class="purecounter"></span>
                        <p>Godziny wsparcia </p>
                    </div>
                </div>

                <div class="col-lg-3 col-md-6 mt-5 mt-lg-0">
                    <div class="count-box">
                        <i class="bi bi-people"></i>
                        <span data-purecounter-start="0" data-purecounter-end="15" data-purecounter-duration="1" class="purecounter"></span>
                        <p>Nasza drużyna</p>
                    </div>
                </div>

            </div>

        </div>
    </section><!-- End Counts Section -->
    <!-- ======= Services Section ======= -->
    <section id="services" class="services section-bg ">
        <div class="container" data-aos="fade-up">

            <div class="section-title">
                <h2>Nasze atuty </h2>
       
            </div>

            <div class="row">
                <div class="col-md-6">
                    <div class="icon-box" data-aos="fade-up" data-aos-delay="100">
                        <i class="bi bi-briefcase"></i>
                        <h4>Zaaganżowanie</h4>
                        <p>Naszym priorytetetem jest pomoc,z pieczołowitością zajmujejmy każdym problem naszych klientów</p>
                    </div>
                </div>
                <div class="col-md-6 mt-4 mt-md-0">
                    <div class="icon-box" data-aos="fade-up" data-aos-delay="200">
                        <i class="bi bi-card-checklist"></i>
                        <h4>Profesjonalizm i wiedza</h4>
                        <p>Jesteśmy grupą specjalistów posiadających wieloletnie doświadczenie. Nieustannie szkolimy się i podnosimy nasze kwalifikacje.</p>
                    </div>
                </div>
                <div class="col-md-6 mt-4 mt-md-0">
                    <div class="icon-box" data-aos="fade-up" data-aos-delay="300">
                        <i class="bi bi-bar-chart"></i>
                        <h4> Najepsi Współpracają z nami </h4>
                        <p>Współpracujemy z wielona firmami oraz podmiotami którzy są znani na całym świecie </p>
                    </div>
                </div>
                <div class="col-md-6 mt-4 mt-md-0">
                    <div class="icon-box" data-aos="fade-up" data-aos-delay="400">
                        <i class="bi bi-binoculars"></i>
                        <h4>Pomoc Prawna</h4>
                        <p>W naszej grupie jest prawdnik który rozwieje wszelkie państwa wątpliości z zakresu prawa unijnego </p>
                    </div>
                </div>
                <div class="col-md-6 mt-4 mt-md-0">
                    <div class="icon-box" data-aos="fade-up" data-aos-delay="500">
                        <i class="bi bi-brightness-high"></i>
                        <h4>Kompleksowe biuro </h4>
                        <p>W naszym zakątku prócz pomocy z zakresu dotacjii unijnych mogą państwo wyciszyć się , odprężyc w specjalnie przygotowanym pokoju ciszy </p>
                    </div>
                </div>
                <div class="col-md-6 mt-4 mt-md-0">
                    <div class="icon-box" data-aos="fade-up" data-aos-delay="600">
                        <i class="bi bi-calendar4-week"></i>
                        <h4><a href="#">Natychmiastowe porady </a></h4>
                        <p>Średni czas oczekiwania na spotkanie wynosi 1 dzień</p>
                    </div>
                </div>
            </div>

        </div>
